<VirtualHost *:80>
    ServerName domain.local
    ServerAlias www.domain.local
    DocumentRoot "/Users/username/Documents/projects/folder"
    ErrorLog "/private/var/log/apache2/domain.local-error_log"
    CustomLog "/private/var/log/apache2/domain.local-access_log" common
    <Directory "/Users/username/Documents/projects/folder">
        AllowOverride All
        Require local granted
    </Directory>
</VirtualHost>